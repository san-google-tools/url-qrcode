{
  "name": "San QRCode Gene",
  "manifest_version": 3,

  "background": {
    "service_worker": "server.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["content/index.js"],
      "matches": ["https://*/*", "http://*/*"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}